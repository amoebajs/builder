    "@types/react-reconciler": "^0.18.0",
    "@types/uuid": "^3.4.6",
    "@types/webpack": "^4.41.0",
    "chalk": "^3.0.0",
    "cheerio": "^0.22.0",
    "css-loader": "^3.2.0",
    "fs-extra": "^8.1.0",
    "html-webpack-plugin": "^3.2.0",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "prettier": "^1.19.1",
    "react-reconciler": "0.24.0",
    "reflect-metadata": "^0.1.13",
    "style-loader": "^1.0.1",
    "ts-loader": "^6.2.1",
    "tslib": "^1.10.0",
    "typescript": "^3.7.2",
    "utility-types": "^3.10.0",
    "uuid": "^3.3.3",
    "webpack": "^4.41.2"
  },
