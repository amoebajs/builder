{
  "name": "@amoebajs/builder",
  "version": "0.0.2-alpha.1581344539548",
  "main": "lib/index.js",
  "types": "es/index.d.ts",
  "module": "es/index.js",
  "repository": "https://github.com/amoebajs/builder.git",
  "contributors": [
    "bigmogician <bigmogician@outlook.com>",
    "pigwang <pigw4ng@gmail.com>"
  ],
  "license": "AGPL-3.0-only",
  "scripts": {
    "clear:dist": "npx rimraf dist",
    "clear:scripts-build": "npx rimraf scripts-build",
    "copy:scripts-yaml": "npx copy scripts/**/*.yaml scripts-build/scripts",
    "copy:scripts-src-assets": "npx copy src/assets/**/*.* scripts-build/src/assets",
    "copy:assets": "npx copy src/assets/**/*.* dist/lib/assets && npx copy src/assets/**/*.* dist/es/assets",
    "copy:prepublish": "yarn clear:dist && npx copy README.md dist && npx copy LICENSE dist",
    "compile:commonjs": "npx tsc --project tsconfig.json",
    "compile:esmodule": "npx tsc --project tsconfig.es.json",
    "compile:node": "yarn compile:commonjs && yarn compile:esmodule && yarn copy:assets",
    "demo:react": "cross-env TS_PROJECT=scripts/tsconfig.json npx ts-node scripts/demo-react.tsx",
    "watch": "node scripts/watch.js --output=../basic-modules/node_modules/@amoebajs/builder",
    "build": "yarn copy:prepublish && yarn compile:node",
    "test": "jest --passWithNoTests",
    "pub": "yarn build && yarn test && node scripts/stable.publish.js",
    "rc": "yarn build && yarn test && node scripts/rc.publish.js"
  },
  "dependencies": {
    "@bonbons/di": "2.0.0-beta.3",
    "@types/cheerio": "^0.22.15",
    "@types/copy-webpack-plugin": "^5.0.0",
